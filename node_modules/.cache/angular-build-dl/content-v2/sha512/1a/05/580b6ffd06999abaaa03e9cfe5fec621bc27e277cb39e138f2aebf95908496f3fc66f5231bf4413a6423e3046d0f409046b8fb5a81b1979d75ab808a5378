 \", this.frm.btnStyle )\n\n\n  }\n  fontSizeChange( event: any ) {\n      this.frm.btnStyle = this.frmSrv.styleParse( this.frm.btnStyle, 'fntSize', this.fontSize + '' );\n      this.mService.produce( this.frm );\n  }\n  fontFamilyChange( event: any ) {\n      this.frm.btnStyle = this.frmSrv.styleParse( this.frm.btnStyle, 'fntFamily', this.selectedFont );\n      this.mService.produce( this.frm );\n  }\n}\n","<div class=\"nav-content nav-color\">\n\t<div class=\"head\">\n\t\tButton Properties <span class=\"right-nav-icon\"><i\n\t\t\tclass=\"fa fa-times\" aria-hidden=\"true\"\n\t\t\t(click)=\"openCloseRightPanel()\"></i></span>\n\t</div>\n\t<div class=\"mat-nav\">\n\t\t\t\n\t\t\t\t\n\t\n\t\t<div *ngIf=\" frm && actvieFormField\">\n\t\t\t<mat-tab-group mat-align-tabs=\"center\" class=\"nav-content tab-header\"\n\t\t\t\tdisablePagination=\"false\"> \n\t\t\t<mat-tab label=\"GENERAL\" class=\"tab-menu\">\n\t\t\t<div style=\"height: calc(100vh - 140px);\">\n\t\t\t<perfect-scrollbar>\n\t\t\t<div class=\"tab-content\">\n\t\t\t\t<div class=\"tab-row\">\n\t\t\t\t\t\t<span class=\"tab-property-label\">Button Label</span>\n\t\t\t\t\t\t<div class=\"mat-nav\">\n\t\t\t\t\t\t\t<mat-form-field appearance=\"outline\"> \n\t\t\t\t\t\t\t\t<input matInput name=\"fname\" [(ngModel)]=\"actvieFormField.name\"> \n<!-- \t\t\t\t\t\t\t\t<mat-label class=\"tab-label-text\">Field Name</mat-label>  -->\n\t\t\t\t\t\t\t</mat-form-field>\n\t\t\t\t\t\t</div>\n\n\t\t\t\t\n\t\t\t\t</div>\n\t\t\t\t<div class=\"tab-row\">\n\t\t\t\t\t<span class=\"tab-property-label\">Label On Button Press</span>\n\t\t\t\t\t<div class=\"mat-nav\">\n\t\t\t\t\t\t<mat-form-field appearance=\"outline\">\n\t\t\t\t\t\t \t<input matInput name=\"fname\" [(ngModel)]=\"actvieFormField.title\"> \n<!-- \t\t\t\t\t\t \t<mat-label class=\"tab-label-text\">Field Place Holder</mat-label>  -->\n\t\t\t\t\t\t </mat-form-field>\n\t\t\t\t\t</div>\n\t\t\t\t</div>\n\t\t\t\t\n\t\t\t\t<div class=\"property-row\">\n\t\t\t\t\t<div class=\"property-row