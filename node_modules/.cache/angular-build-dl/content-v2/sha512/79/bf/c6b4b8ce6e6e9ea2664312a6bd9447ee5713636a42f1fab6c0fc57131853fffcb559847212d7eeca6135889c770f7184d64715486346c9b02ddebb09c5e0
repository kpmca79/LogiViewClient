const _c1 = function () { return ["/createuser"]; };
class UserlistComponent {
    // tslint:disable-next-line:max-line-length
    constructor(dialog, router, route, userService) {
        this.dialog = dialog;
        this.router = router;
        this.route = route;
        this.userService = userService;
        this.users = [];
    }
    ngOnInit() {
        this.users = [];
        this.userService.getAllUsers().subscribe(res => {
            console.log("user resoponse===>", res);
            let userresponse = res;
            this.users = userresponse.data;
            console.log('userlist users = ' + this.users);
        });
    }
    deleteUser(id) {
        console.log('got id for delete=' + id);
        this.userService.deleteUser(id);
        this.userService.getAllUsers().subscribe(res => {
            let userdata = res;
            this.users = userdata.data;
        });
        this.router.navigateByUrl('/userlist');
    }
    openModal(id) {
        const dialogConfig = new _angular_material_dialog__WEBPACK_IMPORTED_MODULE_3__["MatDialogConfig"]();
        dialogConfig.disableClose = true;
        dialogConfig.autoFocus = true;
        dialogConfig.data = {
            id: 1,
            title: 'Delete User',
            message: 'Are you sure you want to delete user ?'
        };
        const dialogRef = this.dialog.open(_confirm_dialog_confirm_dialog_component__WEBPACK_IMPORTED_MODULE_4__["ConfirmDialogComponent"], dialogConfig);
        dialogRef.afterClosed().subscribe(result => {
            if (result) {
                this.deleteUser(id);
            }
        });
    }
}
