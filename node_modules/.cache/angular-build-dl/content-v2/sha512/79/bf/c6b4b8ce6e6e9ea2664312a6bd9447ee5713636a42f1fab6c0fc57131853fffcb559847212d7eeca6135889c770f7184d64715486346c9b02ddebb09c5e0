        console.log("show called", e.target.innerHTML);
        this.s = !this.s;
        if (this.s)
            this.ngOnInit();
    }
    generateHash() {
        this.frmSrv.getPaymentHash(this.paymentDetail).subscribe(data => { this.paymentDetail = data.data; console.log(data); }, error => { console.log(error); });
    }
