class FullnameComponent {
    constructor(mService, frmSrv) {
        this.mService = mService;
        this.frmSrv = frmSrv;
        this.operator = ['=', '!=', 'Contains', '!Contains', 'Starts with', '!Starts with', 'Ends with', '!Ends with', '>', '>=', '<', '<=', 'Specified', '!Specified'];
    }
    ngOnInit() {
        this.icons = this.frmSrv.getMatIcons();
    }
    openCloseRightPanel() {
        let obj = { id: 'close' };
        console.log("9999999999999999999999999999");
        this.mService.produce(obj);
    }
}
