        this.dialog = dialog;
        this.mService = mService;
        this.snackBar = snackBar;
        this.surl = "http://localhost:4200/form/5efa60a7b3f0da1be4221f32";
        this.furl = "http://localhost:4200/login";
        this.captchav2SiteKey = "6LfMx64ZAAAAACQ6nRCglIwPacx3AraqI7vLBFSB";
        this.isBuild = false;
        this.isLive = false;
        this.config = {};
        this.inputStyle = "standard";
        //    inputStyle="outline";
        //    inputStyle="";
