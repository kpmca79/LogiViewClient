        Object(_angular_cdk_drag_drop__WEBPACK_IMPORTED_MODULE_2__["moveItemInArray"])(this.formField, event.previousIndex, event.currentIndex);
    }
    openCloseRightPanel() {
        let obj = { id: 'close' };
        this.mService.produce(obj);
    }
    dataURLtoFile(dataurl) {
        const arr = dataurl.split(',');
        const mime = arr[0].match(/:(.*?);/)[1];
        const bstr = atob(arr[1]);
        let n = bstr.length;
        const u8arr = new Uint8Array(n);
        while (n--) {
            u8arr[n] = bstr.charCodeAt(n);
        }
        let fileName = 'Signature-' + this.frmSrv.getRandom() + '.png';
        const imageFile = new File([u8arr], fileName, { type: "png"
        });
        return imageFile;
    }
    onSubmit($event, field) {
        $event.target.innerText = field.title;
        $event.target.disabled = true;
        let isWait = false;
        setTimeout(() => { $event.target.innerText = field.name; $event.target.disabled = false; }, 1000);
        console.log("onsubmit call mode is ", this.mode);
        if (this.mode == "build" || this.mode == "preview") {
            console.log('Returning from submit at mode is not live mode=', this.mode);
            return;
        }
        console.log("inside submit");
        let isError = false;
        //        console.log( "Today new==>", this.formField )
        this.formField.forEach(field => {
            if (field.frmControl.invalid) {
                isError = true;
                console.log(field.frmControl.invalid);
                return;
            }
            if (field.subfields) {
                field.subfields.forEach(subfield => {
                    if (subfield.frmControl && subfield.frmControl.invalid) {
                        isError = true;
                        console.log(subfield.frmControl.invalid);
                        return;
                    }
                });
            }
        });
        if (!isError) {
            this.respJson.formID = this.formID;
            this.respJson.IpAddress = this.publicIPAddr;
            this.respJson.resTime = this.datePipe.transform(new Date(), 'dd-MM-yyyy HH:mm:ss zzzz');
            console.log("inside submit2");
            this.formField.forEach(field => {
                if (field.type != "section" && field.type != "submit") {
                    if (field.type == 'signature') {
                        console.log("Signatue component found");
                        console.log("Signatue image=", field.signImage);
                        isWait = true;
                        let f = this.dataURLtoFile(field.signImage);
                        console.log("Fiile name=", f.name);
                        this.frmSrv.uploadResource(f).subscribe(data => {
                            let result = data;
                            console.log("Signature uploaded successfully id=", result.fileid);
                            field.signature = result.fileid;
                            field.signature = "<a href='" + "http://localhost:8080/api/file/" + result.fileid + "'>" + field.name + "</a><br/>";
                            console.log("Signature link in response=", field.signature);
                            field.signImage = null;
                            isWait = false;
                            this.respJson[field.name] = field.signature;
                        }, error => {
                            console.log("Signature uploaded failed id=", error);
                            isWait = false;
                        });
                    }
                    let value = field.frmControl.value;
                    if (field.type == "date-picker")
                        value = this.datePipe.transform(value, 'dd-MM-yyyy HH:mm:ss zzzz');
                    //                 if(field.fname)
                    //                     this.respJson[field.fname]=value;
                    else if (field.type == "fullname") {
                        if (field.subfields) {
                            field.subfields.forEach(subfield => {
                                if (subfield.visible) {
                                    this.respJson[subfield.name] = subfield.frmControl.value;
                                    console.log("fullname subfield: ", subfield.name);
                                    console.log("fullname value: ", subfield.frmControl.value);
                                }
                            });
                        }
                    }
                    else if (field.type == "survey") {
                        console.log("survey found===============>", field.survey.question);
                        if (field.survey.questions) {
                            field.survey.questions.forEach(q => {
                                console.log("survey found===q.name============>", q.name);
                                console.log("survey found===q.name============>", q.selectedValue);
                                if (q.selectedValue) {
                                    this.respJson[q.name] = q.selectedValue;
                                }
                            });
                        }
                    }
                    else if (field.type == "address") {
                        if (field.subfields) {
                            field.subfields.forEach(subfield => {
                                if (subfield.visible) {
                                    this.respJson[subfield.name] = subfield.frmControl.value;
                                }
                            });
                        }
                    }
                    else if (field.type == "time") {
                        if (field.subfields) {
                            let selTime = '';
                            selTime = field.subfields[0].frmControl.value; //hour
                            selTime = selTime + ":" + field.subfields[1].frmControl.value; //min
                            selTime = selTime + " " + field.subfields[2].frmControl.value; //am/pm
                            this.respJson[field.fname] = selTime;
                        }
                    }
                    else if (field.type == "phone") {
                        if (field.subfields) {
                            if (field.subfields[0] && field.subfields[0].frmControl)
                                this.respJson[field.subfields[0].name] = field.subfields[0].frmControl.value;
                            if (field.subfields.length > 1 && field.subfields[1] && field.subfields[1].frmControl)
                                this.respJson[field.subfields[1].name] = field.subfields[1].frmControl.value;
                        }
                    }
                    else if (field.type == "upload") {
                        if (field.submitValue) {
                            this.respJson[field.name] = field.submitValue;
                        }
                    }
                    else if (field.type == "rating") {
                        this.respJson[field.name] = this.currentRate;
                    }
                    else if (field.type == "email") {
                        this.respJson[field.name] = value;
                    }
                    else if (field.fname)
                        this.respJson[field.fname] = value;
                    else if (field.name)
                        this.respJson[field.name] = value;
                    console.log("CHECKING FOR EMAL FIELD----------->", field);
                }
            });
            if (isWait)
                setTimeout(() => { console.log("waiting for data to complete before submit"); this.saveFormAfterSignatureUpdate(); }, 2000);
            else
                this.saveFormAfterSignatureUpdate();
            //            console.log( "todaynew1111", this.respJson );
        }
    }
    saveFormAfterSignatureUpdate() {
        let paymentDetail;
        let prdInfo = "";
        let prdInfoResp = "";
        let isPayment = false;
        if (this.frm.formFields)
            this.frm.formFields.forEach(field => {
                if (field.type == 'productlist' && field.productList && field.productList.total > 0) {
                    isPayment = true;
                    console.log("Processing payment for ", field.type);
                    paymentDetail = new app_model_PaymentDetail__WEBPACK_IMPORTED_MODULE_13__["PaymentDetail"]();
                    paymentDetail.amount = field.productList.total.toFixed(2);
                    paymentDetail.firstname = this.respJson["First name"] ? this.respJson["First name"] : "Anonymous";
                    paymentDetail.email = this.respJson["Email"] ? this.respJson["Email"] : "Anonymous@xyz.com";
                    let index = 1;
                    if (field.productList.products)
                        field.productList.products.forEach(prd => {
                            if (prd.quantity > 0) {
                                prdInfo = prdInfo + prd.title + "  ";
                                prdInfoResp = prdInfoResp + index + ". " + prd.title + "(Qnty:" + prd.quantity + ")<br/>";
                                index += 1;
                            }
                        });
                    paymentDetail.productinfo = prdInfo ? prdInfo : "No Product";
                    paymentDetail.surl = this.surl;
                    paymentDetail.furl = this.furl;
                    this.doPayment(paymentDetail).subscribe(data => {
                        console.log("Payment data successfully received", data);
                        if (data.txnStatus) {
                            this.respJson["Transaction Product"] = prdInfoResp;
                            this.respJson["Transaction Amount"] = field.productList.total.toFixed(2);
                            this.respJson["Transaction Currency"] = field.productList.currency ? field.productList.currency : 'USD';
                            this.respJson["Transaction ID"] = data.txnid ? data.txnid : '';
                            this.respJson["Transaction Status"] = data.status;
                            this.respJson["Transaction Mode"] = data.mode ? data.mode : '';
                            this.respJson["Transaction Error"] = data.error_Message ? data.error_Message : '';
                            if (data.status != "success")
                                return;
                            this.saveFormResponse();
                        }
                    }, error => {
                        return;
                        //                        this.saveFormResponse();
                    });
                }
            });
        console.log(this.respJson);
        if (!isPayment)
            this.saveFormResponse();
    }
    saveFormResponse() {
        const formObj = this.frmSrv.saveResponse(this.formID, this.respJson).subscribe(data => {
            //                    console.log( data )
            this.router.navigate(["/form/" + this.formID + "/thanks"]);
        }, error => { console.log(error); return; });
    }
    onFileComplete(data) {
        if (this.mode == "build" || this.mode == "preview")
            return;
        var fileJson = JSON.parse(data);
        this.frm.formFields.forEach(field => {
            if (field.type == 'upload') {
                if (field.name == fileJson.fieldName) {
                    if (!field.submitValue)
                        field.submitValue = "";
                    field.submitValue = field.submitValue + "<a href='" + "http://localhost:8080/api/file/" + fileJson.id + "'>" + fileJson.name + "</a><br/>";
                }
            }
        });
    }
    //    METHODS FOR SIGNATURE COMPONENT
    //    END METHODS FOR SIGNATURE COMPONENT
    //PAYUMONEY PAYMENT METHODS
    doPayment(paymentDetail) {
        return new rxjs__WEBPACK_IMPORTED_MODULE_12__["Observable"](observer => {
            this.frmSrv.getPaymentHash(paymentDetail).subscribe(data => {
                paymentDetail = data.data;
                console.log(data);
                bolt.launch(paymentDetail, { responseHandler: function (BOLT) {
                        console.log("Transaction with id :", paymentDetail.txnid, " is ", BOLT.response.txnStatus);
                        observer.next(BOLT.response);
                        observer.complete();
                    }
                });
            }, error => { console.log(error); });
            //        if(!paymentDetail.hash)
            //        {
            //            console.log("Error getting hash");
            //            observer.error("Not able to generate hash value");
            //            observer.complete();
            //        }
        });
    }
    generateHash() {
    }
    launchBOLT() {
        console.log("inside bolt");
        //        hi();
        //        console.log($('#bolt'));
        //     
    }
}
