class LoginComponent {
    constructor(app, router) {
        this.app = app;
        this.router = router;
        this.error = false;
        this.credentials = { username: 'keyur', password: 'keyur' };
    }
    login() {
        this.app.authenticate(this.credentials, () => { this.router.navigateByUrl('/dashboard'); });
        this.error = !this.app.authenticated;
        return false;
    }
    ngOnInit() {
    }
}
