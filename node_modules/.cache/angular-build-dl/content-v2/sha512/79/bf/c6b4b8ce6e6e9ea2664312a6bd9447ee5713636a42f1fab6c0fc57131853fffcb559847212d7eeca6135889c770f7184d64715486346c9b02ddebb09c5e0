class ImageSelectionDialogComponent {
    constructor(stdSrv, data, dialogRef) {
        this.stdSrv = stdSrv;
        this.data = data;
        this.dialogRef = dialogRef;
        this.supportedSizeInMB = 1;
        this.msg = "Upload file or drag and drop file here";
        this.pubmsg = "Upload file from public URL";
        this.err = "info";
        this.loading = false;
        this.imgURL = "/api/file/";
        this.selectedTab = 0;
    }
    ngOnInit() {
        this.frm = this.data.frm;
        if (this.frm && this.frm.formImages && this.frm.formImages.length > 0) {
            this.selectedTab = 2;
        }
    }
    onFileSelected() {
        const inputNode = document.querySelector('#bgFile');
        let size = 0;
        //button loading reference https://medium.com/@dkreider09/how-to-add-a-spinner-to-an-angular-material-button-the-simplest-way-ever-69e2f7005f29
        this.loading = true;
        if (typeof (FileReader) !== 'undefined') {
            const reader = new FileReader();
            reader.onload = (e) => { this.srcResult = e.target.result; };
            reader.readAsArrayBuffer(inputNode.files[0]);
            size = inputNode.files[0].size / 1014;
            if (size > 1024) {
                this.msg = "File size greater than 1 MB is not allowed";
                this.err = "info err";
                this.loading = false;
                return;
            }
            else {
                this.msg = "Uploading file...";
                this.err = "info prog";
            }
            const studentsObservable = this.stdSrv.uploadResource(inputNode.files[0]).subscribe(dt => {
                let result = dt;
                this.data.fileid = result.fileid;
                //                      this.notifyBGImageChange(result.fileid)
                console.log('File uploaded onFileSelected successfully ID is = ', this.data.fileid);
                let data = { 'fileid': this.data.fileid };
                this.dialogRef.close({ 'data': data });
            }, error => {
                this.msg = error;
                this.err = "info err";
            });
        }
    }
    useImage(imageId) {
        let data = { 'fileid': imageId, 'frm': this.frm };
        this.dialogRef.close({ 'data': data });
    }
    removeMe(imageID) {
        const index = this.frm.formImages.indexOf(imageID);
        if (index !== -1)
            this.frm.formImages.splice(index, 1);
    }
    uploadFromURL() {
        console.log(this.imageURL1);
        this.loading = true;
        this.pubmsg = "Uploading file...";
        this.err = "info prog";
        const imageObservable = this.stdSrv.downloadImage(this.imageURL1)
            .subscribe(data => {
            //          console.log("Inside get data--> ",data);
            let fileType = data.type;
            //type:"image/jpg"
            if (fileType.includes("image/")) {
                var extension = fileType.substring(fileType.indexOf("/") + 1);
                console.log("file extentions is ", extension);
                var blob = new Blob([data], { type: fileType });
                var fileName = "image-" + new Date().valueOf() + "." + extension;
                console.log("fileName is ", fileName);
                var file = new File([blob], fileName, { type: fileType });
                const uploadObservable = this.stdSrv.uploadResource(file).
                    subscribe(data => {
                    if (data) {
                        let result = data;
                        let fileID = result.fileid;
                        let data1 = { 'fileid': fileID };
                        this.dialogRef.close({ 'data': data1 });
                        this.loading = false;
                    }
                }, (err) => { this.pubmsg = err; this.err = "info err"; this.loading = false; });
            }
            else {
                this.pubmsg = "Not supported filetype for background image :" + fileType;
                this.loading = false;
                this.err = "info err";
            }
        }, (err) => {
            this.pubmsg = err;
            this.loading = false;
            this.err = "info err";
        });
    }
}
ImageSelectionDialogComponent.ɵfac = function ImageSelectionDialogComponent_Factory(t) { return new (t || ImageSelectionDialogComponent)(_angular_core__WEBPACK_IMPORTED_MODULE_0__["ɵɵdirectiveInject"](app_services_form_service__WEBPACK_IMPORTED_MODULE_1__["FormService"]), _angular_core__WEBPACK_IMPORTED_MODULE_0__["ɵɵdirectiveInject"](_angular_material_dialog__WEBPACK_IMPORTED_MODULE_2__["MAT_DIALOG_DATA"]), _angular_core__WEBPACK_IMPORTED_MODULE_0__["ɵɵdirectiveInject"](_angular_material_dialog__WEBPACK_IMPORTED_MODULE_2__["MatDialogRef"])); };
