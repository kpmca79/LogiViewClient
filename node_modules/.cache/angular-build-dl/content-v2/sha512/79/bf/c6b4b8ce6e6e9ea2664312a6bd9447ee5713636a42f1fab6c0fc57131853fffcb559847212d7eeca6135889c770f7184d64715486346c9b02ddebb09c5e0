class InlineEditComponent {
    constructor(popover) {
        this.popover = popover;
        this._value = '';
        /** Form model for the input. */
        this.comment = '';
    }
    /** Overrides the comment and provides a reset value when changes are cancelled. */
    get value() { return this._value; }
    set value(x) {
        this.comment = this._value = x;
    }
    // tslint:disable-next-line:use-life-cycle-interface
    ngOnInit() {
        // subscribe to cancellations and reset form value
        if (this.popover) {
            this.popover.closed.pipe(Object(rxjs_operators_filter__WEBPACK_IMPORTED_MODULE_2__["filter"])(val => val == null))
                .subscribe(() => this.comment = this.value || '');
        }
    }
    onSubmit() {
        if (this.popover) {
            this.popover.close(this.comment);
        }
    }
    onCancel() {
        if (this.popover) {
            this.popover.close();
        }
    }
}
