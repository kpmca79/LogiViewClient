class DataGridComponent {
    //END VARIABLE DECLARATION
    constructor(config, dateP, highcharts, sanitizer) {
        this.config = config;
        this.dateP = dateP;
        this.highcharts = highcharts;
        this.sanitizer = sanitizer;
        this.pageSize = 10;
        this.reqSelection = false;
        this.hidencols = [''];
        this.loadChartData = new _angular_core__WEBPACK_IMPORTED_MODULE_1__["EventEmitter"]();
        this.charts = [];
        this.allowDrag = false;
        this.displayedColumns = [];
        this.pageSizeOption = [this.pageSize];
        this.dataColumns = [];
        this.selection = new _angular_cdk_collections__WEBPACK_IMPORTED_MODULE_5__["SelectionModel"](true, []);
        this.panelOpenState = false;
        // images = [1].map(() => '/assets/Lighthouse.jpg');
        //  https://images.pexels.com/photos/952670/pexels-photo-952670.jpeg?cs=srgb&dl=background-black-colors-952670.jpg&fm=jpg
        this.images = [10].map(() => '/assets/img/red3.png');
        this.chartAvail = false;
        this.message = "";
        config.interval = 1000;
        config.wrap = true;
        config.keyboard = false;
        config.pauseOnHover = true;
        config.showNavigationIndicators = true;
        config.showNavigationArrows = true;
    }
    ngOnInit() {
        this.charts.push(null);
        this.pageSizeOption = [this.pageSize];
        console.log("!!!!! " + JSON.stringify(this.data));
        this.data;
        this.dataSource = new _angular_material_table__WEBPACK_IMPORTED_MODULE_4__["MatTableDataSource"](this.data);
        this.dataSource.paginator = this.paginator;
        this.dataSource.sort = this.sort;
        this.setDisplayColumns();
        let chartOptions = this.highcharts.getChartOptionObj("Fruit chart", "column", ["apple", "banana", "orange"], "Fruits", "Fruits consumed", null, null, [{ name: "Fruits", data: [5, 3, 4] }], true, 45, 10);
