class CreateuserComponent {
    constructor(router, route, userService) {
        this.router = router;
        this.route = route;
        this.userService = userService;
        this.imageUrl = './assets/img/faces/marc.jpg';
        this.userdetail = { id: '', firstname: '', lastname: '', email: '', password: '', profilePic: null, aboutMe: '',
            designation: '', organisation: '' };
    }
    ngOnInit() {
    }
    createProfile() {
        console.log(this.userdetail.firstname);
        if (this.imageToUpload == null) {
        }
        this.userService.createUser(this.userdetail, this.imageToUpload);
        this.router.navigateByUrl('/userlist');
    }
    showImage(files) {
        this.imageToUpload = files.item(0);
        console.log('showimagecalled' + this.imageToUpload.name);
        // tslint:disable-next-line:prefer-const
        let reader = new FileReader();
        reader.onload = (event) => {
            console.log('showimagecalled' + event.target.result);
            this.imageUrl = event.target.result;
        };
        reader.readAsDataURL(this.imageToUpload);
    }
}
