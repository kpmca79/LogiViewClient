class FormInboxComponent {
    constructor(srv, dialog, router, route) {
        this.srv = srv;
        this.dialog = dialog;
        this.router = router;
        this.route = route;
        this.isLoaded = false;
        this.title = 'mattab';
        this.leftNavOpen = true;
        this.colProperty = [];
        this.rows = [1, 2, 3, 4, 5, 6, 7, 8, 9];
        this.dispColumns = [];
        this.message = "this is from parent";
        this.chartData = [];
        this.hidencols = ['resp_country', 'resp_state', 'resp_city', 'latitude', 'longitude', 'IpAddress', 'resTime'];
        this.dispColNew = [];
        this.colors = ['#f56ec5', '#fbebb5', '#D3FED4', '#D0F0FE', '#EDE2FE', '#FEE2D5', '#C2F5E9'];
        this.curColIndex = 2;
        this.visible = true;
        this.selectable = true;
        this.removable = true;
        this.separatorKeysCodes = [_angular_cdk_keycodes__WEBPACK_IMPORTED_MODULE_5__["ENTER"], _angular_cdk_keycodes__WEBPACK_IMPORTED_MODULE_5__["COMMA"]];
        this.fruitCtrl = new _angular_forms__WEBPACK_IMPORTED_MODULE_6__["FormControl"]();
        this.fruits = [];
        this.allFruits = [
            { name: 'To Do', color: '#f56ec5' },
            { name: 'In Progress', color: '#fbebb5' },
            { name: 'Done', color: '#D3FED4' },
        ];
        this.CallFruits = ['To Do', 'In Progress', 'Done'];
        this.filteredFruits = this.fruitCtrl.valueChanges.pipe(Object(rxjs_operators__WEBPACK_IMPORTED_MODULE_7__["startWith"])(null), Object(rxjs_operators__WEBPACK_IMPORTED_MODULE_7__["map"])((fruit) => fruit ? this._filter(fruit) : this.allFruits.slice()));
    }
    //  public downloadPDF():void {
    //      let DATA = this.htmlData.nativeElement;
    //      let doc = new jspdf('p','pt', 'a4');
